@using Dar_e_Arqam_pwd_isb.Models;
@model AllClasses
@{
    ViewBag.Title = "View Parent Record";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<script src="~/contents/js/script_14.js"></script>
<link href="~/contents/css/style.css" rel="stylesheet" />
<script type="text/javascript">
      $(document).ready(function () {
          if ('@TempData["msg"]' != "") {
              $("#dialog-msg").dialog('open');
          }
          $("#Parents_tabs").tabs();
      });
</script>

<div class="body_right_top_area">
<div class="p_page_head">
<div class="p_page_head_top"><p> > Parents</p></div>
<div class="p_page_head_left"><h2 class="parents">Parents</h2></div>
<div class="p_page_head_right">
<div class="p_page_head_right_search">
@using (Html.BeginForm("Index", "parents", FormMethod.Get))
{
<div class="p_page_head_right_search_input"> @Html.TextBoxFor(m => m.Search_key, new { placeholder="Type anything to search the required parent .... " }) </div>
<div class="p_page_head_right_search_btn"> <input type="submit" name="submit" value="Search" /> </div>
<div class="clear"></div>    
}
</div>
</div>
<div class="clear"></div>
</div>
<div id="dialog-form"></div>
<!--Success Message-->
<div id="dialog-msg" style="display: none">
    <p class="db_msg">
        @TempData["msg"]!
    </p>
</div>

<!--Error-->
 <div id="dialog-error" style="display: none">
    <p class="db_msg">
        Something Wrong With Your Internet Connection , Please Check Your Internet Connetion Settings !
    </p>
</div>
</div>

<div class="body_right_bot_area">

<div class="p_page_head_bot_links">
<a href="/parents/active">All Active Students's Parents</a>
<a href="/parents/old">All Old Student's Parents</a>
<a href="/parents/trash">Trash</a>
<div class="clear"></div>
</div>

<div id="Parents_tabs">
  <ul>
    <li><a href="#tabs-1">Basic Info</a></li>
    <li><a href="#tabs-2">Documents</a></li>
    <li><a href="#tabs-3">Login Details</a></li>
    <li><a href="#tabs-4">Web Settings</a></li>
</ul>
<div id="tabs-1">

<h3 class="page_tabs_main_headings"> Parent Basic Info Of Student</h3> 
<div class="page_contents_detail_opt">
<div class="page_contents_detail_opt_dlt"><a href="">Delete</a></div>
<div class="page_contents_detail_opt_edit"><a href="/parents/edit/@Model.Student.db_Id" class="UpdateExistingParent">Edit</a></div>
<div class="clear"></div>
</div>          

<div class="page_contents_info_boxes">
<p class="page_contents_detail_h"> <b> Student Reg ID </b> :</p> 
<p class="page_contents_detail">@Model.Student.Aplicnt_Id</p>
</div>

<div class="page_contents_info_boxes">
<p class="page_contents_detail_h"> <b> Father Name </b> :</p> 
<p class="page_contents_detail">@Model.Student.Aplicnt_f_name</p>
</div>

<div class="clear"></div>

<div class="page_contents_info_boxes">
<p class="page_contents_detail_h"> <b> Occupation </b> :</p> 
<p class="page_contents_detail">@Model.Student.Aplicnt_f_ocptn</p>
</div>




<div class="page_contents_info_boxes">
<p class="page_contents_detail_h"> <b> Title </b> :</p> 
<p class="page_contents_detail">@Model.Student.Aplicnt_f_title</p>
</div>

    <div class="clear"></div>

<div class="page_contents_info_boxes">
<p class="page_contents_detail_h"> <b> Cell Number </b> :</p> 
<p class="page_contents_detail">@Model.Student.Aplicnt_f_cell</p>
</div>


<div class="page_contents_info_boxes">
<p class="page_contents_detail_h"> <b> Name Of Business </b> :</p> 
<p class="page_contents_detail">@Model.Student.Aplicnt_f_bsns_name</p>
</div>

    <div class="clear"></div>

<div class="page_contents_info_boxes">
<p class="page_contents_detail_h"> <b> Business Address </b> :</p> 
<p class="page_contents_detail">@Model.Student.Aplicnt_f_bsns_addr</p>
</div>




<div class="page_contents_info_boxes">
<p class="page_contents_detail_h"> <b> Email </b> :</p> 
<p class="page_contents_detail">@Model.Student.Aplicnt_f_email </p>
</div>

    <div class="clear"></div>

<div class="page_contents_info_boxes">
<p class="page_contents_detail_h"> <b> Phone (landline) </b> :</p> 
<p class="page_contents_detail">@Model.Student.Aplicnt_f_phone</p>
</div>



<div class="page_contents_info_boxes">
<p class="page_contents_detail_h"> <b> Mother Name </b> :</p> 
<p class="page_contents_detail">@Model.Student.Aplicnt_m_name</p>
</div>

    <div class="clear"></div>

<div class="page_contents_info_boxes">
<p class="page_contents_detail_h"> <b> Mother Cell Number </b> :</p> 
<p class="page_contents_detail">@Model.Student.Aplicnt_m_cell</p>
</div>


<div class="page_contents_info_boxes">
<p class="page_contents_detail_h"> <b> Mother Phone (Landline) </b> :</p> 
<p class="page_contents_detail">@Model.Student.Aplicnt_m_ldln</p>
</div>

<div class="clear"></div>
  
</div>

<div id="tabs-2">
    <h3 class="page_tabs_main_headings">Documents</h3>
    
<p class="db_msg">Currently this service is down  , Sorry for inconvenience . </p>

</div>

<div id="tabs-3">
    <h3 class="page_tabs_main_headings">Login Details</h3>
    
<p class="db_msg">Currently this service is down  , Sorry for inconvenience . </p>

</div>
<div id="tabs-4">
    <h3 class="page_tabs_main_headings">Web Settings</h3>
    
